		{
			"name": "Class, Function, Control",
			"scope": [
				"keyword.control",
				"storage.type",
				"constant.language"
			],
			"settings": {
				"foreground": "#29B1B1"
			}
		},
		{
			"name": "Numerics",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#4DD5D5"
			}
