	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"editorRuler.foreground",
				"editorIndentGuide.background",
				"editorIndentGuide.activeBackground",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#589C2D"
			}
		},
		{
			"name": "Green Non-Italic",
			"scope": [
				"heading.3"
			],
			"settings": {
				"foreground": "#589C2D"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"heading.1"
			],
			"settings": {
				"foreground": "#DFB74D"
			}
		},
		{
			"name": "Class, Function, Control",
			"scope": [
				"keyword.control",
				"storage.type",
				"constant.language",
				"keyword.operator.logical",
				"storage.modifier.v"
			],
			"settings": {
				"foreground": "#3888BA",
                "fontStyle": "bold"
			}
		},
		{
			"name": "Numerics",
			"scope": [
				"constant",
				"markup.heading"
			],
			"settings": {
				"foreground": "#72AFD5"
			}
		},
		{
			"name": "Dictionary Property Name",
			"scope": [
				"support.type.property-name.json",
				"keyword.other.definition.ini",
				"heading.2"
			],
			"settings": {
				"foreground": "#c97c3e"
			}
		},
		{
			"name": "Arguments",
			"scope": [
				"meta.class",
				"meta.function",
				"punctuation.definition.decorator",
				"entity.name.section.group-title.ini",
				"meta.definition.function.v"
			],
			"settings": {
				"foreground": "#c97c3e"
			}
		}
	]
}
