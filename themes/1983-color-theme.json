	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"editorRuler.foreground",
				"editorIndentGuide.background",
				"editorIndentGuide.activeBackground",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#589C2D"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#DFB74D"
			}
		},
		{
			"name": "Class, Function, Control",
			"scope": [
				"keyword.control",
				"storage.type",
				"constant.language",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#3888BA",
                "fontStyle": "bold"
			}
		},
		{
			"name": "Numerics",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#72AFD5"
			}
		},
		{
			"name": "Dictionary Property Name",
			"scope": [
				"support.type.property-name.json",
				"keyword.other.definition.ini"
			],
			"settings": {
				"foreground": "#3888BA"
			}
		},
		{
			"name": "Arguments",
			"scope": [
				"meta.class",
				"meta.function",
				"punctuation.definition.decorator",
				"entity.name.section.group-title.ini"
			],
			"settings": {
				"foreground": "#D67440" //cd3032
			}
		}
	]
}
